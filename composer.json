{
    "name": "palmans/tracker",
    "description": "Simple site visit/statistics tracker for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Eddie Palmans",
            "email": "eddie@palmans.nl"
        }
    ],
    "keywords": [
        "tracker",
        "analytics",
        "visitor",
        "statistics",
        "laravel",
        "Arrtrust\/Tracker"
    ],
    "version": "2.0.0",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.5.10",
        "friendsofphp/php-cs-fixer": "^3.6",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Palmans\\Tracker\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Palmans\\Tracker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Palmans\\Tracker\\TrackerServiceProvider"
            ],
            "aliases": {
                "Tracker": "Palmans\\Tracker\\TrackerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
